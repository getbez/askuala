<h1>New Assessment</h1>

<%= form_for @assessment,url:concept_assessments_path, html: { multipart: true } do |f| %>
    <%= render 'form', f: f, :assessment => @assessment %>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>


<%= link_to 'Back', edit_concept_path(@assessment.concept_id) %>
