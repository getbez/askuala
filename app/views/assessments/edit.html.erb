<h1>Editing Assessment</h1>



<%= form_for @assessment, html: { multipart: true } do |f| %>
    <%= render 'form', f: f, :assessment => @assessment %>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>


<%= link_to 'Show', @assessment %> |
<%= link_to 'Back', edit_concept_path(@assessment.concept_id) %>
